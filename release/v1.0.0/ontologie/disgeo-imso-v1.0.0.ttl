imso-sh:BestuurlijkGebied a sh:NodeShape;
  mim:herkomst "IMDiSGeo";
  rdfs:label "BestuurlijkGebied";
  mim:datumOpname "23-03-2022";
  mim:herkomstDefinitie "EMSO";
  rdfs:comment "Een bestuurlijk gebied is een registratieve ruimte die op basis van wet- of regelgeving als eenheid geldt van bestuurlijke verantwoordelijkheid.";
  mim:toelichting "Dit betreft bijvoorbeeld de gebieden behorende bij de vier formele bestuurslagen uit de Grondwet (Rijk, provincie, waterschap, gemeente), maar kan ook gebieden van bestuurlijke samenwerkingsverbanden met eigen politiek/bestuurlijke verantwoordelijkheid omvatten. Een voorbeeld daarvan betreft de veiligheidsregio’s.";
  sh:name "BestuurlijkGebied";
  mim:indicatieAbstractObject false;
  mim:populatie "Voor objecttypen die deel uitmaken van een (basis)registratie betreft dit de beschrijving van de exemplaren van het gedefinieerde objecttype die in de desbetreffende (basis)-registratie voorhanden zijn.";
  dcterms:subject <http://begrippen.geostandaarden.nl/disgeo/id/begrip/bestuurlijk_gebied>;
  sh:targetClass imso:BestuurlijkGebied;
  sh:property [
      mim:datumOpname "31-05-2022";
      mim:herkomst "IMDisGeo";
      rdfs:label "geometrie";
      mim:toelichting "Het gebied dat bestuurt wordt door een openbaar lichaam heeft een geometrische representatie in de vorm van één of meerdere polygonen (tweedimensionale vlakken).";
      mim:indicatieMaterieleHistorie false;
      mim:identificerend "false";
      mim:herkomstDefinitie "EMSO";
      mim:indicatieAfleidbaar false;
      rdfs:comment "Geometrische representatie van een gebied op land dat door een openbaar lichaam wordt bestuurd.";
      sh:name "geometrie";
      sh:minCount 1;
      mim:patroon "De verzameling van waarden die gegevens van deze attribuutsoort kunnen hebben, oftewel het waardenbereik, uitgedrukt in een specifieke structuur.";
      sh:maxCount 1;
      mim:indicatieFormeleHistorie false;
      sh:path imso:geometrie;
      sh:xone ([
            sh:class gml:MultiSurface
          ] [
            sh:class gml:Surface
          ])
    ], [
      mim:datumOpname "17-08-202";
      mim:herkomst "IMDiSGeo";
      rdfs:label "type";
      mim:toelichting "De enumaratiewaarden van dit attribuut worden afgeleid uit de concrete specialisaties van het objecttype 'Bestuurlijk gebied.";
      mim:indicatieMaterieleHistorie false;
      mim:identificerend "false";
      mim:herkomstDefinitie "IMDiSGeo";
      mim:indicatieAfleidbaar true;
      rdfs:comment "Categorie waartoe het betreffende bestuurlijke gebied behoort.";
      sh:name "type";
      sh:minCount 1;
      mim:patroon "De verzameling van waarden die gegevens van deze attribuutsoort kunnen hebben, oftewel het waardenbereik, uitgedrukt in een specifieke structuur.";
      sh:maxCount 1;
      mim:indicatieFormeleHistorie false;
      sh:path imso:type;
      sh:node imso-sh:TypeBestuurlijkGebied
    ], [
      rdfs:comment "De geometrische afbakening van het bevattendGebied, waarbinnen de geometrie van het bevatteGebied ,zich moet bevinden, en/of mee moet samenvallen.";
      mim:datumOpname "19-08-2022";
      mim:toelichting "Afgeleide van 'ligtIn'-relaties van de concrete specialisaties van het objecttype 'Bestuurlijk gebied'.";
      mim:herkomstDefinitie "IMDiSGeo";
      sh:path imso:ligtInBestuurlijkGebied;
      mim:indicatieMaterieleHistorie false;
      sh:name "ligtInBestuurlijkGebied";
      rdfs:label "ligtInBestuurlijkGebied";
      mim:indicatieAfleidbaar true;
      mim:herkomst "IMDiSGeo";
      sh:nodeKind sh:IRI;
      mim:indicatieFormeleHistorie false;
      sh:class imso:BestuurlijkGebied
    ], [
      rdfs:comment "Relatie die aangeeft dat het gebied bestuurt wordt door de bestuurder.";
      sh:path imso:wordtBestuurdDoor;
      sh:minCount 1;
      mim:indicatieMaterieleHistorie false;
      sh:name "wordtBestuurdDoor";
      rdfs:label "wordtBestuurdDoor";
      mim:indicatieAfleidbaar true;
      sh:maxCount 1;
      sh:nodeKind sh:IRI;
      mim:indicatieFormeleHistorie false;
      sh:class imso:OpenbaarLichaam
    ] .

